<form method="post" action="{{ path('home_delete', {'id': home.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ home.id) }}">
    <button class="btn btn-outline-danger mt-3"><i class="far fa-trash-alt"></i></button>
</form>
